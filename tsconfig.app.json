{
"extends": "@vue/tsconfig/tsconfig.dom.json",
"include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
],
"exclude": [
    "src/**/__tests__/*"
],
"compilerOptions": {
    "types": ["gapi", "gapi.auth2"],
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
